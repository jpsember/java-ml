{
    "project_type" : "yolo",

    "label_data_type" : "float32",

    "alpha" : 0.1,

    "layers" :[

          {"type" :"conv", "filters": 16, "stride" : [2,2] },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 32, "stride" : [2,2]  },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 64, "stride" : [2,2]   },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 128, "stride" : [2,2]  },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 256, "stride" : [2,2]  },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 128 },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 256 },
          {"type" :"leaky_relu"},


          // Reduce the depth of the volume, since it will be fully-connected
          // to the YOLO output layer
          //
          {"type" :"conv", "filters": 512 },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 512 },
          {"type" :"leaky_relu"},

          {"type":"yolo"},
        ],

        "model_config" : {
              "image_size" : [160,96],   // 5x3 blocks
              "image_channels" : 3,
              "category_count" : 1,
              "block_size" : [32,32],
              "anchor_boxes_pixels" : [[40,30]],
              "lambda_coord" : 5.0,
              "lambda_noobj" : 0.5
        },

        //"special_option" : "pixel_alignment",

  }
