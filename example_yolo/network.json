{

    "project_type" : "yolo",

    "alpha" : 0.1,

    "layers" :[

          {"type" :"conv", "filters": 16 },
          {"type" :"leaky_relu"},
          {"type" :"maxpool"},

          {"type" :"conv", "filters": 32 },
          {"type" :"leaky_relu"},
          {"type" :"maxpool"},

          {"type" :"conv", "filters": 64 },
          {"type" :"leaky_relu"},
          {"type" :"maxpool"},

          {"type" :"conv", "filters": 128 },
          {"type" :"leaky_relu"},
          {"type" :"maxpool"},

          {"type" :"conv", "filters": 128 },
          {"type" :"leaky_relu"},
          {"type" :"maxpool"},

          // Reduce the depth of the volume, since it will be fully-connected
          // to the YOLO output layer
          //
          {"type" :"conv", "filters": 32 },
          {"type" :"leaky_relu"},

          {"type":"yolo"},
        ],

        "model_config" : {
              "image_size" : [416,416],
              "image_channels" : 3,
              "category_count" : 2,
              "block_size" : [32,32],
              "anchor_boxes_pixels" : [[40,60]],
              "lambda_coord" : 5.0,
              "lambda_noobj" : 0.5,
              "neighbor_factor" : 0.90,
        }
  }
