          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 512 },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 512 },
          {"type" :"leaky_relu"},

          {"type" :"conv", "filters": 512 },
          {"type" :"leaky_relu"},

          {"type":"yolo"},
        ],

        "model_config" : {
              "image_size" : [64,32],   // 4x2 blocks
              "image_channels" : 3,
              "category_count" : 1,
              "block_size" : [16,16],
              "anchor_boxes_pixels" : [[18,18]],
              "lambda_coord" : 5.0,
              "lambda_noobj" : 0.5
        }
  }
