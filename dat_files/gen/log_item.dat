enum DataType;

fields {

  // Name of tensor (e.g. variable name)
  //
  string name;

  DataType data_type;

  *int shape;

  // A unique id distinguishing this item from others (e.g., which family
  // this belongs to)
  //
  int unique_id;

  // If nonzero, item is one of a family set, and should be buffered
  // until the entire family has arrived
  //
  int family_size;

  // If family_size > 0, this is the slot within the family
  //
  int family_slot;
}
