fields {

  File target_dir = "generated_images";
  int image_total = 20;
  int seed = 1965;

  ? NeuralNetwork network;
  File network_path;

  string categories = "AB";

  AugmentationConfig augmentation_config;

  // If nonzero, generates random colored lines
  //
  int noise_factor = 30;

  File inspection_dir;

  int max_objects = 4;

  // Plots solid colors for each category and nothing else
  //
  bool obvious_mode;

  // If > 0, limits choice of font from set of this size
  //
  int font_limit;

  // If > 0, limits choice of colors from set of this size
  //
  int color_limit;

  // If true, generates pixels whose values represent the x,y,channel coordinates
  //
  bool pixel_order_mode;
}
